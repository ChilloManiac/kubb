{
  "$schema": "@kubb/core/schemas/config.json",
  "root": ".",
  "input": {
    "path": "./petStore.yaml"
  },
  "output": {
    "path": "./src/gen"
  },
  "logLevel": "info",
  "plugins": [
    [
      "@kubb/swagger",
      {
        "output": "schemas",
        "validate": true
      }
    ],
    [
      "@kubb/plugin-ts",
      {
        "output": "models"
      }
    ],
    [
      "@kubb/plugin-tanstack-query",
      {
        "output": "hooks"
      }
    ]
  ]
}
