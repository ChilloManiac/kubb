{
  "name": "@kubb/ui",
  "version": "3.7.0",
  "description": "Kubb ui",
  "keywords": [
    "typescript",
    "plugins",
    "kubb",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-labs/kubb.git",
    "directory": "packages/ui"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {}
  },
  "files": [
    "src",
    "static",
    "dist",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup && vite build",
    "start": "vite & tsup --watch",
    "preview": "vite preview",
    "clean": "npx rimraf ./dist",
    "lint": "bun biome lint .",
    "lint:fix": "bun biome lint --apply-unsafe .",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../.github/canary.sh && node ../../scripts/build.js canary && pnpm publish --no-git-check",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.0",
    "@hono/zod-openapi": "^0.19.2",
    "@kubb/core": "workspace:*",
    "hono": "^4.7.5",
    "zod": "^3.24.2",
    "@tanstack/react-query": "^5.69.0",
    "@heroui/button": "^2.2.16",
    "@heroui/code": "^2.2.12",
    "@heroui/dropdown": "^2.3.16",
    "@heroui/input": "^2.4.16",
    "@heroui/kbd": "^2.2.12",
    "@heroui/link": "^2.2.13",
    "@heroui/navbar": "^2.2.14",
    "@heroui/snippet": "^2.2.17",
    "@heroui/switch": "^2.2.14",
    "@heroui/system": "2.4.12",
    "@heroui/theme": "2.4.12",
    "@heroui/use-theme": "2.1.6",
    "@react-aria/visually-hidden": "3.8.20",
    "@react-types/shared": "3.28.0",
    "clsx": "2.1.1",
    "framer-motion": "11.15.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.23.0",
    "tailwind-variants": "0.3.0",
    "tailwindcss": "3.4.16"
  },
  "devDependencies": {
    "@types/node": "20.5.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "postcss": "8.4.38",
    "prettier": "3.3.3",
    "@kubb/config-ts": "workspace:*",
    "@kubb/config-tsup": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
